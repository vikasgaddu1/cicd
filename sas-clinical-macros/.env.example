# SAS Viya Environment Configuration
# Copy this file to .env and update with your actual values
# DO NOT commit .env file to version control

# Development Environment
VIYA_BASE_URL=https://viya-dev.example.com
VIYA_CLIENT_ID=your-dev-client-id
VIYA_CLIENT_SECRET=your-dev-client-secret
VIYA_USER=your-username
VIYA_PASS=your-password

# Test Environment
VIYA_BASE_URL_TEST=https://viya-test.example.com
VIYA_CLIENT_ID_TEST=your-test-client-id
VIYA_CLIENT_SECRET_TEST=your-test-client-secret

# Production Environment
VIYA_BASE_URL_PROD=https://viya-prod.example.com
VIYA_CLIENT_ID_PROD=your-prod-client-id
VIYA_CLIENT_SECRET_PROD=your-prod-client-secret

# CAS Configuration
CAS_LIBRARY=CLINICAL
CAS_SESSION_NAME=clinical_macros_session
CAS_TIMEOUT=3600

# Compute Context Configuration
COMPUTE_CONTEXT_DEV=Default Compute Context
COMPUTE_CONTEXT_TEST=Test Compute Context
COMPUTE_CONTEXT_PROD=Production Compute Context

# Notification Settings
NOTIFICATION_EMAIL=team@example.com
TEAMS_WEBHOOK_URL=https://outlook.office.com/webhook/your-webhook-url
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your-slack-webhook

# CI/CD Configuration
CI_LOG_LEVEL=INFO
CI_PARALLEL_JOBS=4
CI_TIMEOUT_MINUTES=30
CI_RETRY_ATTEMPTS=3

# Quality Gates
MIN_CODE_COVERAGE=80
MAX_WARNINGS=10
MAX_ERRORS=0
MAX_COMPLEXITY=10

# Feature Flags
ENABLE_PERFORMANCE_TESTS=true
ENABLE_INTEGRATION_TESTS=true
ENABLE_SECURITY_SCAN=true
ENABLE_DOCUMENTATION_BUILD=true

# Python Script Configuration
PYTHON_PATH=python3
SCRIPT_TIMEOUT=300

# Debug Options (set to true only for troubleshooting)
DEBUG_MODE=false
VERBOSE_LOGGING=false
SAVE_INTERMEDIATE_FILES=false